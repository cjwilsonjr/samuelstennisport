= form_for([@racket, @racket.stringsets.build]) do |f|
  = f.label :brand
  = f.select :brand, options_for_select(@brands)
  = f.label :description
  = f.select :description, options_for_select(@descriptions)
  = f.label :game
  = f.select :game, options_for_select(@games)
  = f.label :restring_price
  = f.select :restring_price, options_for_select(@prices)
  = f.submit "Add Strings"
